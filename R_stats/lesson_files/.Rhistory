x<- c(0,9,18,27)
y<- c(40,60,80,100)
library(tidyverse)
library(broom)
ggplot(data = data.frame(x,y),
aes(x = x, y =y))+
geom_point()
source("~/.active-rstudio-document", echo=TRUE)
model1<-lm(y~x)
model2<-lm(y~sqrt(x))
coef(model1)
ggplot(data = data.frame(x,y),
aes(x = x, y =y))+
geom_point()+
geom_smooth(method = "lm", formula = "y~x")
model2 %>% augment() %>%
ggplot(data = .,
aes(x = x, y =y))+
geom_point()+
geom_smooth(method = "lm", formula = "y~x")+
geom_lines(aes(x = x, y = .fitted))
model2 %>% augment() %>%
ggplot(data = .,
aes(x = x, y =y))+
geom_point()+
geom_smooth(method = "lm", formula = "y~x")+
geom_line(aes(x = x, y = .fitted))
